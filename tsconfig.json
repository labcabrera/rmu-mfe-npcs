{
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "esnext",
    "target": "esnext",
    "allowJs": true,
    "checkJs": false,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "strict": false,
    "resolveJsonModule": true,
  },
  "include": [
    "src/**/*"
  ]
}